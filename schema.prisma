// This file is automatically generated by Keystone, do not modify it manually.
// Modify your Keystone config when you want to change this.

datasource sqlite {
  url      = env("DATABASE_URL")
  provider = "sqlite"
}

generator client {
  provider = "prisma-client-js"
  output   = "node_modules/.prisma/client"
}

model About {
  id      String @id @default(cuid())
  name    String @unique @default("About Page")
  content String @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
}

model BigPictureImage {
  id               String       @id @default(cuid())
  bigPictureImages BigPicture[] @relation("BigPicture_images")
  image            String?
  imageName        String       @default("")
  altText          String       @default("")
}

model BigPicture {
  id      String            @id @default(cuid())
  name    String            @unique @default("Big Picture Page")
  content String            @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  images  BigPictureImage[] @relation("BigPicture_images")
}

model Community {
  id     String @id @default(cuid())
  name   String @unique @default("Community Page")
  values String @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
}

model EventImage {
  id          String  @id @default(cuid())
  eventImages Event[] @relation("Event_images")
  image       String?
  imageName   String  @default("")
  altText     String  @default("")
  caption     String  @default("Caption (optional)")
}

model Event {
  id               String       @id @default(cuid())
  name             String       @default("")
  key              String       @unique @default("")
  thumbnail        String?
  thumbAltText     String       @default("")
  eventDate        DateTime
  registrationLink String       @default("")
  address          String       @default("")
  blurb            String       @default("")
  content          String       @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  images           EventImage[] @relation("Event_images")
}

model Filter {
  id                     String      @id @default(cuid())
  name                   String      @default("")
  type                   String
  section                String?
  from_MediaItem_filters MediaItem[] @relation("MediaItem_filters")
  from_Studio_filters    Studio[]    @relation("Studio_filters")
}

model HomeSlide {
  id         String  @id @default(cuid())
  homeSlides Home[]  @relation("Home_slides")
  image      String?
  altText    String  @default("")
  quote      String  @default("")
}

model Home {
  id     String      @id @default(cuid())
  name   String      @unique @default("Home Page")
  intro  String      @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  slides HomeSlide[] @relation("Home_slides")
}

model MediaImage {
  id                 String      @id @default(cuid())
  mediaImages        MediaItem[] @relation("MediaImage_mediaImages")
  mediaGalleryImages MediaItem[] @relation("MediaImage_mediaGalleryImages")
  image              String?
  imageName          String      @default("Image")
  altText            String      @default("")
  caption            String      @default("Caption")
}

model MediaItem {
  id                          String       @id @default(cuid())
  title                       String       @default("")
  key                         String       @unique @default("")
  thumbnail                   String?
  shortDescription            String       @default("")
  filters                     Filter[]     @relation("MediaItem_filters")
  content                     String       @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  images                      MediaImage[] @relation("MediaImage_mediaImages")
  galleryImages               MediaImage[] @relation("MediaImage_mediaGalleryImages")
  videos                      String?
  file_filename               String?
  file_filesize               Int?
  from_Studio_associatedMedia Studio[]     @relation("Studio_associatedMedia")
}

model NewsImage {
  id         String     @id @default(cuid())
  newsImages NewsItem[] @relation("NewsImage_newsImages")
  image      String?
  imageName  String     @default("Image")
  altText    String     @default("")
  caption    String     @default("Caption (optional)")
}

model NewsItem {
  id           String      @id @default(cuid())
  title        String      @default("")
  key          String      @unique @default("")
  thumbnail    String?
  thumbAltText String      @default("")
  publishDate  DateTime
  blurb        String      @default("")
  body         String      @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  images       NewsImage[] @relation("NewsImage_newsImages")
}

model Person {
  id          String  @id @default(cuid())
  name        String  @default("")
  title       String  @default("")
  image       String?
  blurb       String  @default("")
  remembrance String  @default("")
  content     String  @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
}

model StudioImage {
  id           String   @id @default(cuid())
  studioImages Studio[] @relation("Studio_photos")
  image        String?
  imageName    String   @default("")
  altText      String   @default("")
  caption      String   @default("Caption")
}

model Studio {
  id              String        @id @default(cuid())
  name            String        @default("")
  thumbnail       String?
  blurb           String        @default("")
  key             String        @unique @default("")
  filters         Filter[]      @relation("Studio_filters")
  content         String        @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  photos          StudioImage[] @relation("Studio_photos")
  associatedMedia MediaItem[]   @relation("Studio_associatedMedia")
}